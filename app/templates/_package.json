{
  "name": "<%= libraryName %>",
  "version": "0.0.0",
  "description": "<%= libraryDescription %>",
  "main": "lib/index.js",
  "unpkg": "dist/<%= libraryNameCamelized %>.min.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build:lib": "cross-env BABEL_ENV=umd NODE_ENV=development webpack",
    "build:lib:min": "cross-env BABEL_ENV=umd NODE_ENV=production webpack",
    "build:bundle": "cross-env BABEL_ENV=umd FOLDER=dist NODE_ENV=development webpack",
    "build:bundle:min": "cross-env BABEL_ENV=umd FOLDER=dist NODE_ENV=production webpack",
    "build": "npm run clean && npm run build:lib:min && npm run build:bundle && npm run build:bundle:min",
    "watch": "npm run build:lib -- --watch",
    "start": "webpack-dev-server --config example/webpack.config.js",
    "test": "jest --watchAll",
    "test:once": "jest",
    "lint": "eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm run build",
    "preversion": "npm run build",
    "prepublish": "npm run build"
  },
  "jest": {
    "testRegex": "(/test/.*|(\\.|/))\\.js?$",
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "keywords": [
    "react",
    "component"
  ],
  "peerDependencies": {
    "react": "^0.14.6 || 15.x.x || 16.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme": "^2.4.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^18.1.0",
    "node-sass": "^4.5.3",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
}
