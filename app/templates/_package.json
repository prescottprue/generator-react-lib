{
  "name": "<%= libraryName %>",
  "version": "0.0.0",
  "description": "<%= libraryDescription %>",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/<%= libraryNameCamelized %>.min.js",
  "scripts": {
    "clean": "rimraf es lib dist",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack",
    "build": "npm run clean && npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "watch": "npm run build:commonjs -- --watch",
    "start": "webpack-dev-server --config example/webpack.config.js",
    "test": "jest --watchAll",
    "test:once": "jest",
    "lint": "eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm run build",
    "preversion": "npm run build && git add -A lib"
  },
  "jest": {
    "testRegex": "(/test/.*|(\\.|/))\\.js?$",
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "keywords": [
    "react",
    "component"
  ],
  "peerDependencies": {
    "react": "^0.14.6 || 15.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^18.1.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.13.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.2.0"
  }
}
